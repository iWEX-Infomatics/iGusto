 "html": "<div style=\"text-align: center; border: 2px solid #2c3e50; border-radius: 15px; padding: 30px 25px; width: 380px; margin: 20px auto; font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif; background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%); box-shadow: 0 4px 15px rgba(0,0,0,0.1);\">\r\n    \r\n    <!-- Room Info -->\r\n    <div style=\"background: linear-gradient(135deg, #667eea 0%, #764ba2 100%); color: white; padding: 15px; border-radius: 10px; margin-bottom: 20px; box-shadow: 0 3px 10px rgba(102,126,234,0.3);\">\r\n        <h2 style=\"margin: 0; font-size: 28px; font-weight: 700; letter-spacing: 1px;\">ROOM {{ doc.room_number }}</h2>\r\n    </div>\r\n    \r\n    <p style=\"margin: 15px 0; font-size: 15px; color: #555; font-weight: 500; line-height: 1.6;\">\r\n        <span style=\"display: block; font-size: 16px; color: #333; margin-bottom: 5px;\">\ud83d\udcf1 Scan QR Code</span>\r\n        <span style=\"color: #667eea;\">to Order a Service</span>\r\n    </p>\r\n    \r\n    <!-- QR Code -->\r\n    {% set qr_html = frappe.db.get_value(\"Room\", doc.name, \"room_id\") %}\r\n    <div style=\"margin: 20px 0; padding: 15px; background: white; border-radius: 12px; box-shadow: 0 2px 12px rgba(0,0,0,0.08); display: inline-block;\">\r\n        {{ qr_html or \"\" | safe }}\r\n    </div>\r\n    \r\n    <!-- Divider -->\r\n    <div style=\"height: 2px; background: linear-gradient(to right, transparent, #667eea, transparent); margin: 25px 0;\"></div>\r\n    \r\n    <!-- Company Info -->\r\n    {% set company_name = frappe.db.get_single_value(\"Global Defaults\", \"default_company\") %}\r\n    {% if company_name %}\r\n        {% set company = frappe.get_doc(\"Company\", company_name) %}\r\n        \r\n        <!-- Company Logo -->\r\n        {% set logo_file = frappe.db.sql(\"\"\"\r\n            SELECT file_url \r\n            FROM `tabFile` \r\n            WHERE attached_to_doctype = 'Company' \r\n            AND attached_to_name = %s \r\n            AND is_private = 0\r\n            LIMIT 1\r\n        \"\"\", (company.name,), as_dict=1) %}\r\n        \r\n        {% if logo_file and logo_file[0] and logo_file[0].file_url %}\r\n            <div style=\"margin: 15px 0;\">\r\n                <img src=\"{{ logo_file[0].file_url }}\" alt=\"Company Logo\" width=\"90\" height=\"90\" style=\"object-fit: contain; border-radius: 50%;  padding: 8px; background: white; box-shadow: 0 2px 8px rgba(102,126,234,0.2);\">\r\n            </div>\r\n        {% else %}\r\n            <div style=\"margin: 15px 0; width: 90px; height: 90px; border-radius: 50%; background: linear-gradient(135deg, #667eea, #764ba2); display: inline-flex; align-items: center; justify-content: center; color: white; font-size: 36px; font-weight: bold; box-shadow: 0 2px 8px rgba(102,126,234,0.3);\">\r\n                {{ company.company_name[0] if company.company_name else \"?\" }}\r\n            </div>\r\n        {% endif %}\r\n        \r\n        <!-- Company Name -->\r\n        <h3 style=\"margin: 15px 0 10px 0; font-size: 22px; color: #2c3e50; font-weight: 700;\">{{ company.company_name }}</h3>\r\n        \r\n        <!-- Company Address -->\r\n        {% set address_link = frappe.db.get_value(\"Dynamic Link\", \r\n            {\r\n                \"link_doctype\": \"Company\",\r\n                \"link_name\": company.name,\r\n                \"parenttype\": \"Address\"\r\n            }, \r\n            \"parent\") %}\r\n        \r\n        {% if address_link %}\r\n            {% set company_address = frappe.get_doc(\"Address\", address_link) %}\r\n            {% if company_address.is_your_company_address == 1 %}\r\n                <div style=\"font-size: 13px; margin: 12px 0; color: #555; line-height: 1.7; padding: 12px; background: rgba(102,126,234,0.05); border-radius: 8px; border-left: 3px solid #667eea;\">\r\n                    \ud83d\udccd \r\n                    {% if company_address.address_line1 %}{{ company_address.address_line1 }}<br>{% endif %}\r\n                    {% if company_address.address_line2 %}&nbsp;&nbsp;&nbsp;&nbsp;{{ company_address.address_line2 }}<br>{% endif %}\r\n                    &nbsp;&nbsp;&nbsp;&nbsp;{% if company_address.city %}{{ company_address.city }}{% endif %}{% if company_address.state %}, {{ company_address.state }}{% endif %}{% if company_address.pincode %} - {{ company_address.pincode }}{% endif %}\r\n                    {% if company_address.country %}<br>&nbsp;&nbsp;&nbsp;&nbsp;{{ company_address.country }}{% endif %}\r\n                </div>\r\n            {% endif %}\r\n        {% endif %}\r\n        \r\n        <!-- Contact Info -->\r\n        <div style=\"font-size: 13px; margin-top: 15px; padding: 12px; background: white; border-radius: 8px; box-shadow: 0 1px 5px rgba(0,0,0,0.08);\">\r\n            {% if company.phone_no %}\r\n                <div style=\"margin: 6px 0; color: #667eea; font-weight: 500;\">\r\n                    <span style=\"font-size: 16px;\">\ud83d\udcde</span> {{ company.phone_no }}\r\n                </div>\r\n            {% endif %}\r\n            {% if company.email %}\r\n                <div style=\"margin: 6px 0; color: #667eea; font-weight: 500;\">\r\n                    <span style=\"font-size: 16px;\">\u2709\ufe0f</span> {{ company.email }}\r\n                </div>\r\n            {% endif %}\r\n        </div>\r\n    {% endif %}\r\n    \r\n    <!-- Footer decoration -->\r\n    <div style=\"margin-top: 20px; font-size: 10px; color: #999; letter-spacing: 0.5px;\">\r\n        \u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\u2501\r\n    </div>\r\n</div>",
